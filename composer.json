{
    "name": "adobi/laravel-masked-db-dump",
    "description": "Dump masked information from your database",
    "keywords": [
        "adobi",
        "laravel-masked-db-dump"
    ],
    "homepage": "https://github.com/adobi/laravel-masked-db-dump",
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Dobi",
            "email": "hello@adobi.hu",
            "homepage": "http://adobi.hu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/dbal": "^2.0|^3.0",
        "fakerphp/faker": "1.15",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.12|^7.1",
        "phpunit/phpunit": "^8.0|^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\LaravelMaskedDumper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\LaravelMaskedDumper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeyondCode\\LaravelMaskedDumper\\LaravelMaskedDumpServiceProvider"
            ]
        }
    }
}
